    },
    "dependencies": {
        "@node-wot/binding-coap": "0.8.10",
        "@node-wot/binding-file": "0.8.10",
        "@node-wot/binding-http": "0.8.10",
        "@node-wot/binding-mqtt": "0.8.10",
        "@node-wot/binding-websockets": "0.8.10",
        "@node-wot/core": "0.8.10",
        "@node-wot/td-tools": "0.8.10",
        "@types/lodash": "^4.14.199",
        "ajv": "^8.11.0",
        "commander": "^9.1.0",
        "dotenv": "^8.6.0",
        "lodash": "^4.17.21",
        "vm2": "3.9.18"
    },
    "scripts": {
        "build": "tsc -b",
        "start": "ts-node src/cli.ts",
        "debug": "node -r ts-node/register --inspect-brk=9229 src/cli.ts",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write \"src/**/*.ts\" \"**/*.json\"",
        "test": "mocha --require ts-node/register --extension ts"
    },
    "bugs": {
