{
  "name": "@hidetak/binding-wotfirestore",
  "version": "0.0.5",
  "description": "Firestore binding for node-wot",
  "repository": "https://github.com/hidetak/thingweb.node-wot",
  "main": "dist/wotfirestore.js",
  "scripts": {
    "build": "tsc",
    "rebuild": "tsc & npm rebuild --unsafe-perm --build-from-source",
    "test": "firebase emulators:exec -c test/firebase.json \"npm run fbemutest\" && kill-port 4000 8088 9099",
    "fbemutest": "mocha --require ts-node/register test/**/*.ts --timeout 10000 --exit",
    "codestyle": "standard --pretty"
  },
  "author": "hidetak",
  "license": "EPL-2.0",
  "dependencies": {
    "@node-wot/core": "0.8.0",
    "@node-wot/td-tools": "0.8.0",
    "buffer": "^5.5.0",
    "firebase": "^7.24.0",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@firebase/testing": "^0.20.11",
    "@types/chai": "^4.2.18",
    "@types/chai-as-promised": "^7.1.4",
    "@types/firebase": "^3.2.1",
    "@types/sinon": "^10.0.2",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "firebase-admin": "^9.9.0",
    "firebase-tools": "^9.12.1",
    "kill-port": "^1.6.1",
    "mocha": "^8.4.0",
    "mocha-typescript": "^1.1.17",
    "sinon": "^11.1.1",
    "ts-node": "^8.6.2",
    "typescript": "4.1.3",
    "typescript-standard": "0.3.36",
    "wot-typescript-definitions": "^0.8.0-SNAPSHOT.18"
  }
}
